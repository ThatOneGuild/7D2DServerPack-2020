<configs>
	<append xpath="/blocks">

    <block name="guppyCupRack">
			<property name="Material" value="Mmetal"/>
			<property name="IsTerrainDecoration" value="true"/>	
			<property name="Shape" value="ModelEntity"/>	
			<property name="Model" value="#@modfolder:Resources/guppyCommercialKitchenBlocks.unity3d?guppyCupRack.prefab"/>
			<property name="Place" value="TowardsPlacerInverted"/>			
			<property name="OnlySimpleRotations" value="true"/>
			<property name="Collide" value="movement,melee,bullet,arrow,rocket"/>	
			<property name="CustomIcon" value="guppyCupRack" />
			<property name="Stabilitysupport" value="true"/>
			<property name="CreativeMode" value="Player"/>
			<property name="Stacknumber" value="10"/>
			<property name="Group" value="Basics,Building"/>
			<property name="DescriptionKey" value="torchGroupDesc"/>
			<property name="FilterTags" value="fdecor,felectrical"/>
			<property name="SortOrder1" value="70a0"/>
      <property name="MultiBlockDim" value="1,1,1" /> 
      <drop event="Harvest" name="terrStone" count="0" tool_category="Disassemble"/>
      <drop event="Harvest" name="resourceMechanicalParts" count="0,1" tag="salvageHarvest"/>
      <drop event="Harvest" name="resourceMetalPipe" count="1,2" tag="salvageHarvest"/>
      <drop event="Destroy" name="resourceScrapIron" count="5,10" prob="1"/>
      <drop event="Fall" name="scrapMetalPile" count="1" prob="0.75" stick_chance="1"/>       
		</block>
    
